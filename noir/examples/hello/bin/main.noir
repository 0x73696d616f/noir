    fn main(x : Witness,y : Witness) {
        let k : [2]Witness = [x, y];
        
        priv t = k[0] as u200;
        priv p = k[1] as u200;
        
        priv j = add(t, p);
        priv m = j * k[0] * k[1];

        constrain j <= 25;
        constrain m <= 10;
    }

    fn add(x : Witness,y : Witness){
        x + y
    }